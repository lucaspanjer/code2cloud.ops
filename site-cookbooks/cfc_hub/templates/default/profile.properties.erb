profile.invitationOnlySignUp=<%= node.cfc.hub.invitation_only %>

profile.application.protocol=<%= node.cfc.hub.protocol %>
profile.application.host=<%= cfc_role_address("profile") %>
profile.prefix.path=<%= node.cfc.hub.prefix_path %>
<% if node.cfc.hub.service_proxy_path %>
profile.serviceProxy.path=<%= node.cfc.hub.service_proxy_path %>
<% end %>
ssh.server.publicPort=<%= node.cfc.hub.public_ssh_port %>
ssh.server.port=<%= node.cfc.hub.internal_ssh_port %>

alm.database.password=<%= node.cfc.mysql_pw.profile %>
alm.database.url=jdbc\:<%= node.cfc.server.jdbc_type %>//<%= cfc_role_address("db") %>\:<%= node.cfc.server.jdbc_port %><%= node.cfc.hub.jdbc_path %>
alm.database.username=profile

alm.mailSender.javaMailProperties[mail.smtp.starttls.enable]=<%= node.cfc.mail_sender.starttls %>
alm.mailSender.port=<%= node.cfc.mail_sender.port %>
alm.mailSender.host=<%= node.cfc.mail_sender.host %>
alm.mailSender.username=<%= node.cfc.mail_sender.username %>
alm.mailSender.password=<%= node.cfc.mail_sender.password %>
alm.mailSender.from=<%= node.cfc.mail_sender.from %>

alm.rememberMeServices.key=<%= node.cfc.hub.remember_me_key %>

github.consumerKey=<%= node.cfc.github.consumer_key %>
github.consumerSecret=<%= node.cfc.github.consumer_secret %>

github.textEncryptor.password=ThisIsMyGiantPassword.ItDoesntMakeSenseButThisIsIt
github.textEncryptor.salt=CAFEBABE00DEADBEEF00214748364700FACE0FFEE7

<% for prop in node.cfc.hub.additional_properties %>
<%= prop %>
<% end %>
