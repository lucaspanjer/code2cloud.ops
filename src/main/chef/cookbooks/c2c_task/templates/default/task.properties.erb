alm.rootDir=<%= node.c2c.server.opt %>

profile.application.host=<%= c2c_profile_host %>
profile.application.protocol=<%= node.c2c.hub.protocol %>
profile.prefix.path=<%= node.c2c.hub.prefix_path %>

database.host=<%= c2c_role_address("db") %>

task.database.password=<%= node.c2c.db.task.password %>
task.database.url=jdbc\:<%= node.c2c.server.jdbc_type %>//<%= c2c_role_address("db") %>\:<%= node.c2c.server.jdbc_port %><%= node.c2c.server.jdbc_path %>
task.database.username=<%= node.c2c.db.task.username %>

wiki.database.password=<%= node.c2c.db.wiki.password %>
wiki.database.url=jdbc\:<%= node.c2c.server.jdbc_type %>//<%= c2c_role_address("db") %>\:<%= node.c2c.server.jdbc_port %><%= node.c2c.server.jdbc_path %>
wiki.database.username=<%= node.c2c.db.wiki.username %>

<% for prop in node.c2c.taskAndWiki.additional_properties %>
<%= prop %>
<% end %>
