<% if node["cfc"]["proxy_environment"]["http_proxy"] -%>
HTTP_PROXY=<%= node["cfc"]["proxy_environment"]["http_proxy"] %>:<%= node["cfc"]["proxy_environment"]["http_proxy_port"] %>
http_proxy=<%= node["cfc"]["proxy_environment"]["http_proxy"] %>:<%= node["cfc"]["proxy_environment"]["http_proxy_port"] %>
<%- end %>
<% if node["cfc"]["proxy_environment"]["https_proxy"] -%>
HTTPS_PROXY=<%= node["cfc"]["proxy_environment"]["https_proxy"] %>:<%= node["cfc"]["proxy_environment"]["https_proxy_port"] %>
https_proxy=<%= node["cfc"]["proxy_environment"]["https_proxy"] %>:<%= node["cfc"]["proxy_environment"]["https_proxy_port"] %>
<%- end %>
<% if node["cfc"]["proxy_environment"]["no_proxy_prefix"] -%>
NO_PROXY=<%= node["cfc"]["proxy_environment"]["no_proxy_prefix"] %><%= cfc_role_address("profile") %>,<%= cfc_role_address("profile") %>
no_proxy=<%= node["cfc"]["proxy_environment"]["no_proxy_prefix"] %><%= cfc_role_address("profile") %>,<%= cfc_role_address("profile") %>
<%- else %>
NO_PROXY=<%= cfc_role_address("profile") %>
no_proxy=<%= cfc_role_address("profile") %>
<%- end %>
PATH="/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games"
JAVA_HOME="<%= node.java.java_home %>"
GRAILS_HOME="<%= node.grails.path %>"
