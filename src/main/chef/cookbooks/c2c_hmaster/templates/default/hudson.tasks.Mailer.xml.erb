<?xml version='1.0' encoding='UTF-8'?>
<hudson.tasks.Mailer_-DescriptorImpl>
  <hudsonUrl><%= node.c2c.hub.protocol %>://<%= c2c_role_address("profile") %><%= node.c2c.hub.prefix_path %>/s/${profile.application.identifier}/hudson/</hudsonUrl>
  <adminAddress>Code2Cloud Builder &lt;noreply@tasktop.com&gt;</adminAddress>
  <useSsl><%= node.c2c.mail_sender.starttls %></useSsl>
  <charset>UTF-8</charset>
  <smtpHost><%= node.c2c.mail_sender.host %></smtpHost>
  <smtpAuthUsername><%= node.c2c.mail_sender.username %></smtpAuthUsername>
  <smtpAuthPassword><%= @c2c_mail_sender_password %></smtpAuthPassword>
</hudson.tasks.Mailer_-DescriptorImpl>
